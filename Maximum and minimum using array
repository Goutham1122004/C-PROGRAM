#include<stdio.h>
int main()
{
    int a[100],n,i,min,max;
    printf("ENTER THE NUMBER=");
    scanf("%d",&n);
    printf("VALUES=");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    min=a[0];
    max=a[0];
    for(i=0;i<n;i++)
    {
        if(min>a[i])
        min=a[i];
        if(max<a[i])
        max=a[i];
    }
    printf("ENTER THE MINIMUM VALUE=%d",min);
    printf("ENTER THE MAXIMUM VALUE=%d",max);
    return 0;
}

