#include <stdio.h>

int main ()
{
  float p, n, r, si, count = 1;
  while (count <= 3)
    {
      printf ("\nP=");
      scanf ("%f", &p);
      printf ("N=");
      scanf ("%f", &n);
      printf ("R=");
      scanf ("%f", &r);
      si = (p * n * r) / 100;
      printf ("ANSWER=%f", si);
      count = count + 1;
    }
  return 0;
}
