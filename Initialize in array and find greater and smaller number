#include<stdio.h>
int main()
{
    int a[10]={1,2,3,4,5,6,7,8,9,0},i,min,max;
    min=a[0];
    max=a[0];
    for(i=0;i<10;i++)
    {
        if(a[i]<min)
        min=a[i];
        if(a[i]>max)
        max=a[i];
    }
    printf("ENTER THE MINIMUM VALUE=%d\n",min);
    printf("ENTER THE MAXIMUM VALUE=%d",max);
    return 0;
}
