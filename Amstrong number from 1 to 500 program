#include <stdio.h>

int main()
{
   int i,r,t,y;
   for(i=1;i<=500;i++)
   {
       t=i;
       y=0;
        while(t!=0)
        {
            r=t%10;
            y=(r*r*r)+y;
            t=t/10;
        }
        if(y==i)
        {
            printf("\nIT IS AMSTRONG NUMBER =%d",i);
        }
   }
    return 0;
}
