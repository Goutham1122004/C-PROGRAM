#include<stdio.h>
int main()
{
    int h,t;
    float c;
    printf("HARDNESS=");
    scanf("%d",&h);
    printf("CARBON=");
    scanf("%f",&c);
    printf("TENSILE STRENGTH=");
    scanf("%d",&t);
    if(h>50&&c<0.7&&t>5600)
    {
        printf("GRADE 10");
    }
    else if(h>50&&c<0.7)
    {
        printf("GRADE 9");
    }
    else if(c<0.7&&t>5600)
    {
        printf("GRADE 8");
    }
    else if(h>50&&t>5600)
    {
        printf("GRADE 7");
    }
    else if(h>50||c<0.7||t>5600)
    {
        printf("GRADE 6");
    }
    else
    {
        printf("GRADE 5");
    }
    return 0;
}
