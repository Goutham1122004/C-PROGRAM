#include<stdio.h>
#include<math.h>
int main()
{
    int x,y;
    float r,phi,degree;
    printf("X=");
    scanf("%d",&x);
    printf("Y=");
    scanf("%d",&y);
    r=sqrt(x*x+y*y);
    phi=atan(y/x);
    printf("THE POLAR COORDINATE OF(%d,%d)is(%f%f)\n",x,y,r,phi);
    degree=(180/3.14)*phi;
    printf("THE POLAR COORDINATE OF (%f)is(%f)",r,degree);
    return 0;
    
}
