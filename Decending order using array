#include<stdio.h>
int main()
{
    int a[30],b,i,j,n;
    printf("ENTER THE VALUE=");
    scanf("%d",&n);
    printf("INPUT VALUE=");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=i+1;j<n;j++)
        {
            if(a[i]<a[j])
            {
                b=a[i];
                a[i]=a[j];
                a[j]=b;
            }
        }
    }
    for(i=0;i<n;i++)
    printf("DECENDING ORDER=%d\n",a[i]);
    return 0;
}

