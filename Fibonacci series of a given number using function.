#include<stdio.h>  
void fibonacci(int);  
int main()  
{  
    int l;  
    printf("Enter the number=");  
    scanf("%d",&l);  
    fibonacci(l);  
    return 0;  
}  
void fibonacci(int n)  
{  
    int a=0,b=1,c,i;  
    printf("Fibonacci Series =%d,%d,",a,b);  
    for(i=0;i<=n-2;i++)  
    {  
        c=a+b;  
        a=b;  
        b=c;  
        printf("%d,",c);
    } 
}  
