#include<stdio.h>
#include<math.h>

int main()
{
    float x1,y1,x2,y2,dx,dy,dis;
    printf("Enter the value of x1: ");
    scanf("%f", &x1);
    printf("Enter the value of y1: ");
    scanf("%f", &y1);
    printf("Enter the value of x2: ");
    scanf("%f", &x2);
    printf("Enter the value of y2: ");
    scanf("%f", &y2);
    dx= x2 - x1;
    dy= y2 - y1;
    dis= sqrt(pow(dx, 2) + pow(dy, 2));
    printf("The distance between the two points are= %f", dis);
    return 0;
}
