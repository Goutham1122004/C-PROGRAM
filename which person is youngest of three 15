#include <stdio.h>
int main()
{
  int a,b,c;
  printf("G0UTHAM AGE=");
  scanf("%d",&a);
   printf("VIMAL AGE=");
  scanf("%d",&b);
   printf("MUGILAN AGE=");
  scanf("%d",&c);
  if(a<b&&a<c)
    printf("GOUTHAM IS A YOUNGEST PERSON");
  else if(b<a&&b<c)
    printf("VIMAL IS A YOUNGEST PERSON");
  else
  printf("MUGILAN IS A YOUNGEST PERSON");
    return 0;
}

