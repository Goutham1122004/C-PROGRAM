#include <stdio.h>
int main()
{
    int a[100],n,i,odd,even;
    printf("ENTER THE NUMBER=");
    scanf("%d",&n);
    printf("ENTER THE ARRAY=");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        if(a[i]%2==0)
           odd++;
        else
           even++;
    }
    printf("ODD NUMBER=%d\n",odd);
    printf("EVEN NUMBER=%d",even);
    return 0;
}

