#include <stdio.h>
int main()
{
    int a[100],n,i,sum=0;
    printf("ENTER THE NUMBER=");
    scanf("%d",&n);
    printf("ARRAY ELEMENTS=");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
        sum=sum+a[i];
    }
     printf("ANSWER=%d",sum);
    return 0;
}
