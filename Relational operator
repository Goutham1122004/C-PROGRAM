#include<stdio.h>
int main()
{
    int a,b;
    printf("A=");
    scanf("%d",&a);
    printf("B=");
    scanf("%d",&b);
    printf("%d==%d is %d",a,b,a==b);
    printf("\n%d<%d is %d",a,b,a<b);
    printf("\n%d<=%d is %d",a,b,a<=b);
    printf("\n%d>%d is %d",a,b,a>b);
    printf("\n%d>=%d is %d",a,b,a>=b);
    printf("\n%d!=%d is %d",a,b,a!=b);
    return 0;
}

