#include<stdio.h>
int main()
{
    int a[100],n,i,max;
    printf("ENTER THE NUMBER=");
    scanf("%d",&n);
    printf("VALUE=");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[0];
    for(i=0;i<n;i++)
    {
        if(max<a[i])
        max=a[i];
    }
    printf("LARGEST VALUE =%d",max);
    return 0;
}

