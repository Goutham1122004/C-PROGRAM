#include <stdio.h>
int main()
{
   int i,j,r,c,a[10][10],b[10][10];
   printf("ROW=");
   scanf("%d",&r);
   printf("COL=");
   scanf("%d",&c);
   for(i=0;i<r;i++)
   {
       for(j=0;j<c;j++)
       {
           scanf("%d",&a[i][j]);
       }
   }
    for(i=0;i<r;i++)
   {
       for(j=0;j<c;j++)
       {
          b[j][i]=a[i][j];
       }
   }
   for(i=0;i<c;i++)
   {
       for(j=0;j<r;j++)
       {
          printf("%d ",b[i][j]);
       }
       printf("\n");
   }

    return 0;
}

