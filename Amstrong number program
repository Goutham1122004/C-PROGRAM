#include <stdio.h>

int main()
{
   int n,r,y,temp;
   printf("N=");
   scanf("%d",&n);
   y=0;
   temp=n;
   while(n>0)
   {
      r=n%10;
      y=(r*r*r)+y;
      n=n/10;
   }
    printf("RESULT=%d",y);
   if(y==temp)
   {
       printf("\nIT IS AMSTRONG NUMBER");
   }
   else
   {
       printf("\nIT IS NOT AMSTRONG NUMBER");
   }
    return 0;
}
