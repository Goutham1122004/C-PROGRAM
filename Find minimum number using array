#include<stdio.h>
int main()
{
    int a[100],n,i,min;
    printf("ENTER THE NUMBER=");
    scanf("%d",&n);
    printf("VALUES");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    min=a[0];
    for(i=0;i<n;i++)
    {
        if(min>a[i])
        min=a[i];
    }
    printf("ENTER THE MINIMUM VALUE=%d",min);
    return 0;
}

